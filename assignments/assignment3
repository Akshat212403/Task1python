def fact(n):
  if n ==0 or n==1:
   return 1

  else: 
   return n*fact(n-1)
num = int(input("enter  number for factorial : "))
print("factorial of",num,"is",fact(num))    




#q2 assignment3

import math
print ("enter any number")
n = int (input ("enter any number"))
print(math.pow(n,2))  
print(math.pi)
print (math.sqrt(n))
print(math.factorial(n))
print(math.gcd(n,10))  
print(math.sin(n))     
print(math.cos(n))
print(math.tan(n))    
